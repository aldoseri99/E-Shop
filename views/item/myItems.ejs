<h1>My Items</h1>
<table class="table item-table">
  <tr>
    <td></td>
    <td>Item Title</td>
    <td>Quantity</td>
    <td>Price</td>
    <td></td>
    <td></td>
  </tr>
  <% items.forEach((item)=>{ %>
  <tr>
    <td>
      <img
        src="../itemImages/<%= item.image[0]%>"
        alt=""
        style="
          width: 150px;
          height: 150px;
          object-fit: cover;
          object-position: bottom;
        "
      />
    </td>
    <td><%= item.name %></td>
    <td><%= item.qty %></td>
    <td><%= item.Price %>BD</td>
    <td>
      <a href="/item/edit?id=<%= item._id %>"
        ><i class="fa-solid fa-pen-to-square"></i
      ></a>
    </td>
    <td>
      <a href="/item/delete?id=<%= item._id %>"
        ><i class="fa-solid fa-trash"></i
      ></a>
    </td>
  </tr>
  <%})%>
</table>
