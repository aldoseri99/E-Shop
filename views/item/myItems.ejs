<style>
  td {
    border: 1px solid black;
    border-collapse: collapse;
    padding: 0px 10px;
  }
</style>

<h1>My Items</h1>
<% items.forEach((item)=>{ %>
<table>
  <tr>
    <td><img src="../itemImages/<%= item.image[0]%>" alt="" /></td>
    <td><%= item.name %></td>
    <td><%= item.qty %></td>
    <td><%= item.Price %></td>
    <td>
      <a href="/item/edit?id=<%= item._id %>"
        ><i class="fa-solid fa-pen-to-square"></i
      ></a>
    </td>
    <td>
      <a href="/item/delete?id=<%= item._id %>"
        ><i class="fa-solid fa-trash"></i
      ></a>
    </td>
  </tr>
</table>
<%})%>
